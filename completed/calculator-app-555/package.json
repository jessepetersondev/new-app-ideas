{
  "name": "agentic-coding-starter-kit",
  "version": "1.1.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run db:migrate && next build",
    "build:ci": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "npx tsx scripts/setup.ts",
    "env:check": "node -e \"require('./src/lib/env.ts').checkEnv()\" || echo 'Run with tsx: npx tsx -e \"import { checkEnv } from './src/lib/env'; checkEnv();\"'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:dev": "drizzle-kit push",
    "db:reset": "drizzle-kit drop && drizzle-kit push"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.106",
    "@openrouter/ai-sdk-provider": "^1.3.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@vercel/blob": "^2.0.0",
    "ai": "^5.0.106",
    "better-auth": "^1.4.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.539.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "latest",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.5.1",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.5",
      "@types/react-dom": "19.2.3"
    }
  }
}
